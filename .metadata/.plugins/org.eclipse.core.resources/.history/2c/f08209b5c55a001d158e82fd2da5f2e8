package Thread;

public class Thread1 extends Thread {
	@Override
	public void run() {
		System.out.println("Hello World " + Thread.currentThread().getName());
	}

	public static void main(String[] args) {
		Thread1 t1 = new Thread1();
		t1.start();
		System.out.println(Thread.currentThread().getName());
		
	}
}
