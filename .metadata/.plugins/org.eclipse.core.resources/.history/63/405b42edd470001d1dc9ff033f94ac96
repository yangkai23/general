package one;

public class Variable {
	static int x;//0
	double a;//0.0
	float f;//0.0
	char c;//     \u0000
	boolean b;   //false
public void demo() {
	System.out.println("Demo");
}
	public void test() {
		int y;
//		System.out.println(y);
		System.out.println(x);
		demo();
	}

	public static void main(String[] args) {
		Variable variable = new Variable();
		variable.test();
	}
}
