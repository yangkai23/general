package one;

import java.util.Scanner;

public class Second {
	public static int sum(int a, int b) {
		return a + b;
	}
	public static int sub(int result, int c) {
		return result-c;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		int result = sum(a, b);
		int ans = sub(result, c);
		System.out.println(result);
		System.out.println(ans);
		sc.close();
	}
}
