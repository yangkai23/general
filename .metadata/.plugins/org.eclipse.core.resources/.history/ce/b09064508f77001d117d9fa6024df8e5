package one;

import java.util.Scanner;

public class ArmstrongNumber {

	static {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter number");
		int num = sc.nextInt(), cont = 0;
		do {
			int d = num % 10;
			cont += d * d * d;
			num /= 10;
		} while (num != 0);
		sc.close();
	}

	public static void main(String[] args) {

	}
}
